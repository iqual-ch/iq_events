{
  "name": "iqual/iq_events",
  "description": "Provides functionality creating and managing events",
  "type": "drupal-custom-module",
  "repositories": [
    {
      "type": "composer",
      "url": "https://github.com/iqual-ch/iq_events"
    }
  ],
  "require": {
    "php": "^8.1",
    "drupal/core": "^9.5 || ^10",
    "drupal/office_hours": "^1.5",
    "drupal/address": "^1.9",
    "drupal/address_formatter": "^2.0",
    "drupal/entity": "^1.2",
    "iqual/iq_entity_browsers": "^3.0",
    "drupal/pagedesigner_block_adaptable": "^3.0 || ^4.0",
    "drupal/field_group": "^3.4",
    "drupal/calendar": "^1.0"
  },
  "extra": {
    "patches": {
      "drupal/address_formatter": {
        "[https://dgo.to/3296528]: Automated Drupal 10 compatibility fixes": "https://git.drupalcode.org/project/address_formatter/-/merge_requests/1/diffs.diff?diff_id=656289"
      }
    }
  }
}
